<?xml version="1.0" encoding="utf-8"?>
<genders>
	<!-- DO NOT CHANGE THE FOLLOWING GENDER ID'S : male, unknown, admin, hidden -->
	<!-- ADMIN ONLY GENDERS SHOULD ALWAYS BE ADDED GROUPED TOGETHER-->
	<!-- EXPLANATIONS FOR EACH ATTRIBUTE : 
			id: general name by which the gender is identified
			label: the name that is displayed in the login area
			iconUrl: the path to the image for a specific gender icon
			adminOnly: whether or not the gender is displayed only to the admin or not
			sortPriority: specifies the priority the gender has when the option sort by genders is selected for the users list in a room 
			outlineColor: specifies the color to be used for the left line element that is drawn in the users list on mouse over a user item -->
	<gender id="male" label="Male" iconUrl="gender_icons/male.png" adminOnly="false" sortPriority="b" outlineColor ="0x577ce5"/>		
	<gender id="female" label="Female" iconUrl="gender_icons/female.png" adminOnly="false" sortPriority="c" outlineColor ="0xe76495"/>
	<gender id="unknown" label="Unspecified" iconUrl="gender_icons/ratherNotSay.png" adminOnly="false" sortPriority="e" outlineColor ="0xe398e41"/>
	
	<gender id="admin" label="Admin" iconUrl="gender_icons/male.png" adminOnly="true" sortPriority="a" outlineColor ="0xe398e41"/>
	<gender id="hidden" label="Hidden" iconUrl="gender_icons/ratherNotSay.png" adminOnly="true" sortPriority="a" outlineColor ="0xe398e41"/>
</genders>